{
  "manifest_version": 3,
  "name": "Fill It AI",
  "description": "Fill It AI is an extension that helps you to fill forms of jobs platforms using AI.",
  "version": "0.1.0",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["https://www.linkedin.com/*", "https://www.indeed.com/*", "https://www.getonboard.com/*"],
  "action": { "default_popup": "index.html" },
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*", "https://www.indeed.com/*", "https://www.getonboard.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
